Teleport to player
Code: [Select]
{    
	if (isPlayer _x && (name _x=="USER")) then {    
          player setPos (position _x);
	}  
} forEach playableUnits;

Teleport player to self
Code: [Select]
{    
	if (isPlayer _x && (name _x=="USER")) then {    
          _x setPos (position player);
	}  
} forEach playableUnits;

